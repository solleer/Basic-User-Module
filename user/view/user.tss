main { content: template("user.xml"); }
html:attr(data-user) { content: 'all'; }
head title, #pageTitle { content: "User Account"; }

main .first_name { content: data(first_name); }
main .last_name { content: data(last_name); }
main .username { content: data(username); }
main .email { content: data(email); }
main .version { content: data(default_version); }
.user [data-user-prop] { content: data(attr(data-user-prop)); }

@import "/Layouts/layout.tss";
