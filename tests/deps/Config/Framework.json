{
    "Config\\Environment" : {
        "constructParams" : [
            "github/framework"
        ]
    },
    "Config\\Modules" : {
        "constructParams" : [
            [
                ""
            ]
        ]
    },
    "Config\\Resources" : {
        "constructParams" : [
            {
                "js" : ["resources/jquery-1.11.3.js", "resources/jquery.validate.js", "resources/jquery-ui-1.11.4.custom/jquery-ui.js",
                    "resources/slick/slick.js", "resources/wickedpicker/src/wickedpicker.js", "resources/monthpicker/MonthPicker.js",
                    "addFontAwesomeLinkForCDN", "resources/custom.js"],
                "css" : ["resources/skeleton/skeleton.css", "resources/slick/slick.css",
                    "resources/slick/slick-theme.css", "resources/jquery-ui-1.11.4.custom/jquery-ui.css",
                    "resources/wickedpicker/stylesheets/wickedpicker.css", "resources/monthpicker/MonthPicker.css",
                    "resources/layout.css", "Modules/Static/static.css"]
            }
        ]
    },
    "Config\\Dice": {
        "constructParams" : [
            ["Config/Dice/Config.json", "Config/Dice/Framework.json"]
        ]
    },
    "*" : {
        "substitutions" : {
            "Dice\\Dice" : {
                "instance" : ["Dice\\Loader\\Json", "load"],
                "params" : [
                    {"instance": ["Config\\Dice", "getDiceFiles"]}
                ]
            }
        }
    },
    "Level2\\Router\\Router" : {
        "call" : [
            ["addRule", [{"instance" : "Config\\Router\\StaticPages"}]],
            ["addRule", [{"instance" : "Config\\Router\\ModuleJson"}]],
            ["addRule", [{"instance" : "Config\\Router\\ModulePhp"}]]
        ]
    }
}
